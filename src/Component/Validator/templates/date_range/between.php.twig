$after = \DateTime::createFromFormat('{{ format }}', '{{ after }}');
$before = \DateTime::createFromFormat('{{ format }}', '{{ before }}');

$date = \DateTime::createFromFormat('{{ format }}', {{ getProperty }});
if ($after > $date || $before < $date) {
    $violationsList[] = new \Kaa\Component\Validator\Violation('{{ class }}', '{{ property }}', '{{ message }}');
}